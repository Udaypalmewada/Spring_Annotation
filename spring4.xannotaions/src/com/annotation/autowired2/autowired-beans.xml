<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

<bean id="chip" class="com.annotation.autowired2.Chip">

<property name="chipName" value="kumar"></property>
<property name="chipno" value="125"></property>
</bean>
<bean id="robot" class="com.annotation.autowired2.Robot"></bean>
<bean id="sensor" class="com.annotation.autowired2.Sensor">
<property name="id" value="124"/>
<property name="sensorName" value="automatic"></property>

</bean>
<bean id="sensor1" class="com.annotation.autowired2.Sensor">
<property name="id" value="1224"/>
<property name="sensorName" value="senautomatic"></property>

</bean>

<context:annotation-config></context:annotation-config>
</beans>
